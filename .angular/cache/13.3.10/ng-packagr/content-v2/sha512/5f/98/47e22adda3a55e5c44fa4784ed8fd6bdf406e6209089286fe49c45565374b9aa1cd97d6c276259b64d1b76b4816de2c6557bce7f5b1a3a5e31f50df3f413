);\r\n        }, of(false)).pipe(skip(1), take(n));\r\n    }\r\n    takeOne(actionType, handler) {\r\n        return this.take(1, actionType, handler);\r\n    }\r\n}\r\n/** @nocollapse */ EventsService.ɵfac = function EventsService_Factory(t) { return new (t || EventsService)(i0.ɵɵinject(i1$1.Store)); };\r\n/** @nocollapse */ EventsService.ɵprov = /** @pureOrBreakMyCode */ i0.ɵɵdefineInjectable({ token: EventsService, factory: EventsService.ɵfac, providedIn: 'root' });\r\n(function () {\r\n    (typeof ngDevMode === \"undefined\" || ngDevMode) && i0.ɵsetClassMetadata(EventsService, [{\r\n            type: Injectable,\r\n            args: [{\r\n                    providedIn: 'root'\r\n                }]\r\n        }], function () { return [{ type: i1$1.Store }]; }, null);\r\n})();\n\nclass EventsModule {\r\n}\r\n/** @nocollapse */ EventsModule.ɵfac = function EventsModule_Factory(t) { return new (t || EventsModule)(); };\r\n/** @nocollapse */ EventsModule.ɵmod = /** @pureOrBreakMyCode */ i0.ɵɵdefineNgModule({ type: EventsModule });\r\n/** @nocollapse */ EventsModule.ɵinj = /** @pureOrBreakMyCode */ i0.ɵɵdefineInjector({ providers: [\r\n        EventEffects,\r\n        EventsService,\r\n    ] });\r\n(function () {\r\n    (typeof ngDevMode === \"undefined\" || ngDevMode) && i0.ɵsetClassMetadata(EventsModule, [{\r\n            type: NgModule,\r\n            args: [{\r\n                    providers: [\r\n                        EventEffects,\r\n                        EventsService,\r\n                    ],\r\n                    exports: []\r\n                }]\r\n        }], null, null);\r\n})();\n\nfunction eventsReducer(state = new Events$1(), action) {\r\n    switch (action.type) {\r\n        case EventActions.ADD_EVENT:\r\n            return state.addEvent(action.payload);\r\n        case EventActions.REMOVE_EVENT:\r\n            return state.removeEvent(action.payload);\r\n        case EventActions.TRIGGER_EVENT:\r\n            return state.triggerEvent(action.