elete() {\r\n        this.delete.emit(this.address);\r\n    }\r\n    onSubmit() {\r\n        this.save.emit(this.valueOut);\r\n    }\r\n    registerOnChange(fn) {\r\n        this.onModelChange = fn;\r\n    }\r\n    registerOnTouched(fn) {\r\n        this.onTouch = fn;\r\n    }\r\n    setDisabledState(isDisabled) { }\r\n    writeValue(value) {\r\n        if (this.debug) {\r\n            console.log('\\nCHANGE Address VALUE');\r\n            console.dir(getValue(build(Address, value)));\r\n        }\r\n        this.value = value;\r\n        this.setValue(build(Address, value));\r\n    }\r\n}\r\n/** @nocollapse */ AddressFormComponent.ɵfac = function AddressFormComponent_Factory(t) { return new (t || AddressFormComponent)(); };\r\n/** @nocollapse */ AddressFormComponent.ɵcmp = /** @pureOrBreakMyCode */ i0.ɵɵdefineComponent({ type: AddressFormComponent, selectors: [[\"iu-address-form\"]], inputs: { address: \"address\", canDelete: \"canDelete\", debug: \"debug\", hasPrimaryAddress: \"hasPrimaryAddress\", manager: \"manager\", requireEffectiveDate: \"requireEffectiveDate\", showName: \"showName\", zipPlus4: \"zipPlus4\" }, outputs: { changes: \"changes\", delete: \"delete\", save: \"save\" }, features: [i0.ɵɵProvidersFeature([ADDRESS_FORM_ACCESSOR]), i0.ɵɵInheritDefinitionFeature, i0.ɵɵNgOnChangesFeature], decls: 34, vars: 14, consts: [[1, \"address-form\", 3, \"formGroup\", \"ngSubmit\"], [\"class\", \"control first-name\", 4, \"ngIf\"], [\"class\", \"control last-name\", 4, \"ngIf\"], [1, \"control\", \"address1\"], [\"matInput\", \"\", \"placeholder\", \"Address 1\", \"type\", \"text\", \"formControlName\", \"address1\"], [\"address1\", \"\"], [\"mat-button\", \"\", \"matSuffix\", \"\", \"mat-icon-button\", \"\", \"aria-label\", \"Clear\", 3, \"click\", 4, \"ngIf\"], [1, \"control\", \"address2\"], [\"matInput\", \"\", \"placeholder\", \"Address 2\", \"type\", \"text\", \"formControlName\", \"address2\"], [\"address2\", \"\"], [1, \"control\", \"city\"], [\"matInput\"